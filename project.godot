run/main_scene="res://Scenes/Levels/mainmenu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0.527595, 0.460219, 0.797345, 1)
boot_splash/show_image=false
config/icon="res://Assets/Art/Player/Title2.png"
boot_splash/minimum_display_time=2385

[autoload]

Signalman="*res://Scripts/Simpletons/signalman.gd"
Gameman="*res://Scripts/Simpletons/gameman.gd"
Mastercummer="*res://Scripts/Simpletons/MasterCreator.gd"
Soundman="*res://Scripts/Simpletons/soundman.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=600
window/size/mode=3
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Assets/Art/CoolCursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotAnnotate/plugin.cfg")

